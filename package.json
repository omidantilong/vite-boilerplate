{
  "name": "vite-starter",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "test": "jest --watch --modulePaths=src",
    "test:staged": "jest --bail --modulePaths=src --findRelatedTests",
    "lint": "npx stylelint \"**/*.scss\"",
    "lint:fix": "npx stylelint \"**/*.scss\" --fix",
    "prepare": "husky install",
    "prettier": "prettier --write"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "include-media": "^1.4.10",
    "msw": "^0.35.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-query": "^3.32.3",
    "react-router-dom": "^6.0.2",
    "stylelint": "^14.1.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended-scss": "^5.0.2",
    "stylelint-order": "^5.0.0",
    "stylelint-prettier": "^2.0.0",
    "stylelint-scss": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/eslint-parser": "^7.16.3",
    "@babel/plugin-proposal-export-default-from": "^7.16.0",
    "@babel/plugin-transform-runtime": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-react": "^7.16.0",
    "@nabla/vite-plugin-eslint": "^1.3.4",
    "@testing-library/jest-dom": "^5.15.0",
    "@testing-library/react": "^12.1.2",
    "@vitejs/plugin-react": "^1.0.0",
    "babel-jest": "^27.3.1",
    "babel-preset-vite": "^1.0.4",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.27.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.3.1",
    "lint-staged": "^12.1.1",
    "prettier": "^2.4.1",
    "sass": "^1.43.4",
    "vite": "^2.6.4",
    "vite-plugin-svgr": "^0.6.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "**/*.{js,jsx}": "jest --bail --modulePaths=src --findRelatedTests",
    "*.{js,jsx}": "eslint --cache --fix",
    "*.{scss,css}": "stylelint --fix",
    "*.{js,jsx,scss,css,md}": "prettier --write"
  }
}
